<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace = "com.biz.dripbag.mapper.UserDAO">
	<select id="selectAll" resultType="userVO">
		select * from tbl_user
	</select>
	
	<select id="findById">
	</select>
	
	<insert id="insert">
		INSERT INTO tbl_user(d_seq, d_email, d_password, d_date, d_roll) 
				VALUES (seq_user.NEXTVAL, #{d_email}, #{d_password}, #{d_date}, #{d_roll})
	</insert>
	
	<update id="update">
	
	</update>
	
	<delete id="delete">
	
	</delete>
	
 </mapper>