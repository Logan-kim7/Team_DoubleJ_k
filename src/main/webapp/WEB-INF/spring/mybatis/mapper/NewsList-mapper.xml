<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace = "com.biz.dripbag.mapper.NewsListDAO">
	<select id="selectAll" resultType="NewsListVO">
		SELECT * FROM tbl_NewsComent
	</select>
	
	<select id="findById" resultType="NewsListVO">
		SELECT * FROM tbl_NewsComent WHERE seq = #{seq}
	</select>
	
	<insert id="insert">
		INSERT INTO tbl_NewsComent(seq, dates, title, img)
			 VALUES (seq_newsR.NEXTVAL, #{dates}, #{title}, #{img})
	</insert>
	
	<update id="update">
	
	</update>
	
	<delete id="delete">
	</delete>
	
 </mapper>